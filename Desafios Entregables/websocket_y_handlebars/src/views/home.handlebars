<header>
    <div class="title">
        <h1 class="title__text">Tienda de productos online</h1>
    </div>
</header>
<main>
    <div class="products">
        {{#each products}}
        <div class="products__title">
            <h2 class="products__title-text">{{this.title}}</h2>
        </div>
        <div class="products__more">
            <p class="products__more-description">{{this.description}}</p>
            <p class="products__more-price">Precio: {{this.price}}</p>
            <p class="products__more-stock">Disponibles: {{this.stock}}</p>
            <img class="products__more-img" src="{{this.thumbnail}}">
        </div>
            <hr>
        {{/each}}
    </div>
</main>
<footer>
    
</footer>
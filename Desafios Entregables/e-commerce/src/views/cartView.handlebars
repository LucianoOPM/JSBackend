<main>
  <div>
    {{log emptyCart}}
    {{#if emptyCart}}
      <h1>Empty Cart</h1>
    {{else}}
      {{#each renderCart}}
        {{#each this.products}}
          <ul>
            <li>
              <h3>Producto: {{product.title}}</h3>
              <p>Descripcion: {{product.description}}</p>
              <p>Costo: {{product.price}}</p>
            </li>
          </ul>
        {{/each}}
      {{/each}}
    {{/if}}
  </div>
</main>